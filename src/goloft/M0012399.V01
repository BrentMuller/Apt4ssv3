*
      SUBROUTINE INTFA2 (FACE,PL,P1,P2,IRC1)
C
      DOUBLE PRECISION FACE(6,3),PL(4),P1(3),P2(3),PI(3),DIST,DIST1
C
      DIST1=-PL(4)
      DIST=0.D0
C
      DO 10 I=1,3
      DIST1=DIST1+PL(I)*P1(I)
   10 DIST=DIST+(P2(I)-P1(I))*PL(I)
C
      DIST=DIST1/DIST
C
      DO 20 I=1,3
   20 PI(I)=P1(I)+(P2(I)-P1(I))*DIST
C
      CALL INTFA1(FACE,PL,PI,IRC1)
C
      RETURN
      END
