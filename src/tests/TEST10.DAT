$$
$$   THIS TEST OK IN SSV1 - 25-JUN-1982
$$               AND SSV2 - 01-OCT-1986
$$
CLPRNT
NOPOST
$$   BOTTOM PROFILE
      PLOW=POINT/-10,0,0
      QLOW=POINT/0,0,3
      RLOW=POINT/10,0,0
$$   TOP PROFILE
      PHI=POINT/-8,10,1
      QHI=POINT/0,10,2
      RHI=POINT/8,10,1
$$   VECTORS POINTING FROM BOTTOM TO TOP
      VP=VECTOR/PLOW,PHI
      VQ=VECTOR/QLOW,QHI
      VR=VECTOR/RLOW,RHI
$$   VECTORS POINTING FROM LEFT TO RIGHT, BOTTOM PROFILE
      VINC=VECTOR/0,0,1
      WPLOW=VECTOR/PLOW,QLOW
      VPLOW=VECTOR/WPLOW,PLUS,VINC
      VQLOW=VECTOR/10,0,0
      WRLOW=VECTOR/QLOW,RLOW
      VRLOW=VECTOR/WRLOW,MINUS,VINC
$$   VECTORS POINTING FROM LEFT TO RIGHT, TOP PROFILE
      WPHI=VECTOR/PHI,QHI
      VPHI=VECTOR/8.,0.,2.
      VQHI=VECTOR/8,0,0
      WRHI=VECTOR/QHI,RHI
      VRHI=VECTOR/8.,0.,-2.
$$   USE A ZERO TWIST VECTOR
      TW=VECTOR/0,0,0
$$   INTRODUCE  COMMON GEOMETRY FOR MILLING
      LBOUN=LINE/PLOW,PHI
      CENLIN=LINE/QLOW,QHI
      RBOUN=LINE/RLOW,RHI
      SETPT=POINT/1,1,3
      YVAL=2.0
      CPLN=PLANE/0,1,0,YVAL
$$   CUTTING PARAMETERS
      CUTTER/1.0,0.5,0,0,0,0,1.0
      INTOL/.001
      OUTTOL/.001
      YINC=0.5
$$   SCULPTURED SURFACE DEFINITION,  SSX2 LANGUAGE (1971 RELEASE)
      SS  =  SSURF/ PATCH, PNTVEC,  $
         PLUS, PLOW,QLOW,VPLOW,VQLOW, PHI,QHI,VPHI,VQHI, $
               VP   ,VQ  ,TW  ,TW   , VP ,VQ , TW  , TW, $
         PLUS, QLOW,RLOW,VQLOW,VRLOW, QHI,RHI,VQHI,VRHI, $
               VQ   ,VR , TW  , TW  , VQ ,VR , TW , TW
$$   CUTTER MOTION COMMANDS FOLLOW
   FROM/SETPT
   GO/CPLN, SS, CENLIN
      INDIRV/-1,0,0
      GOFWD/CPLN,TO,LBOUN
ID1) YVAL=YVAL+YINC
      JPLN=PLANE/0,1,0,YVAL
      GORGT/LBOUN,TO,JPLN
      GORGT/JPLN,RBOUN
      YVAL=YVAL+YINC
      HPLN=PLANE/0,1,0,YVAL
      GOLFT/RBOUN,HPLN
      GOLFT/HPLN,LBOUN
      IF(YVAL-8.9) ID1,ID1,ID2
  ID2) GODLTA/0,0,5
      FINI
