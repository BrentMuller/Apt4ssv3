$$
$$   THIS TEST OK IN SSV1 - 24-JUN-1982
$$               AND SSV2 - 30-SEP-1986
$$
CLPRNT
NOPOST
INTOL/.001
OUTTOL/.001
CUTTER/.5,.25,0,0,0,0,.25
      PZ=POINT/0,0,0
      P1=POINT/0,0,0
      P2=POINT/1,0,1
      P3=POINT/2,0,1
      P4=POINT/3,0,0
      Q1=POINT/0,3,0
      Q2=POINT/1,3,1
      Q3=POINT/2,3,1
      Q4=POINT/3,3,0
      R1=POINT/0,6,0
      R2=POINT/1,6,1
      R3=POINT/2,6,1
      R4=POINT/3,6,0
      VUP=VECTOR/1,0,1
      VDN=VECTOR/1,0,-1
      TW=VECTOR/0,0,0
      VCROS=VECTOR/0,3,0
      PLY3=PLANE/1,0,0,3
      PLY=PLANE/1,0,0,0
      PLY1=PLANE/1,0,0,1
      PLX0=PLANE/0,1,0,0
      PLX6=PLANE/0,1,0,6
      PLX3=PLANE/0,1,0,3
      SETPT=POINT/3,1,2
      SPOLY=SSURF/   PATCH,POLYGN,PLUS,P1,P2,P3,P4,$
                       PZ,PZ,PZ,PZ,PZ,PZ,PZ,PZ,$
                       Q1,Q2,Q3,Q4,PLUS,Q1,Q2,Q3,Q4,$
                       PZ,PZ,PZ,PZ,PZ,PZ,PZ,PZ,$
                       R1,R2,R3,R4
      SPTON=SSURF/PATCH,PNTSON,PLUS,P1,P2,P3,P4,$
                       PZ,PZ,PZ,PZ,PZ,PZ,PZ,PZ,$
                       Q1,Q2,Q3,Q4,PLUS,Q1,Q2,Q3,Q4,$
                       PZ,PZ,PZ,PZ,PZ,PZ,PZ,PZ,$
                       R1,R2,R3,R4
SPNTV=SSURF/PATCH,PNTVEC,PLUS,P1,P4,VUP,VDN,Q1,Q4,VUP,VDN,$
          VCROS,VCROS,TW,TW,VCROS,VCROS,TW,TW,$
                     PLUS,Q1,Q4,VUP,VDN,R1,R4,VUP,VDN,$
          VCROS,VCROS,TW,TW,VCROS,VCROS,TW,TW
      M=MACRO/ASURF
      FROM/SETPT
      GO/PLX0,ASURF,PLY
      INDIRV/-1,0,0
      TLRGT,GORGT/PLY,PLX6
      GORGT/PLX6,ON,PLY1
      TLON,GORGT/PLY1,ON,PLX3
      GOLFT/PLX3,PLY3
      TLRGT,GORGT/PLY3,PLX0
      GOTO/SETPT
     END
      TERMAC
      CALL/M,ASURF=SPOLY
      CALL/M,ASURF=SPTON
      CALL/M,ASURF=SPNTV
      FINI
