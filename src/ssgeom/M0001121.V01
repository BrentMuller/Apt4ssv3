*
      SUBROUTINE TRANSP(A,B,N)
      IMPLICIT DOUBLE PRECISION(A-H,O-Z)
      DIMENSION A(N,N),B(N,N)
C---     TRANSPOSE A NXN MATRIX  CALLED A AND PLACE IN B
C---     NOTE THAT B MAY BE THE SAME AS A
      DO 10 I=1,N
      J=I
      DO 20 K=1,J
      T=A(I,K)
      B(I,K)=A(K,I)
      B(K,I)=T
   20 CONTINUE
   10 CONTINUE
      RETURN
      END
