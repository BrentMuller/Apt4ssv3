       TERMAC
 $$
 $$
 $$ TEST-1  1.0 DIA CUTTER BALL END  OFF SET      TLAXIS/0,0,1
 $$
       CUTTER/1,.5
      GOTO/4.5,6.2,4
       TLAXIS/0,0,1
       CALL/MA
       GOLFT/HL13
       GOLFT/APL14
       GORGT/HL12
       GORGT/APL15
       GOLFT/HL13
       GOLFT/APL16
       GORGT/HL12
