**** FILENAME = [MM001.TESTS]TEST48.DAT        ****
PARTNO TEST48,PARMF- UND NUMF-TEST OF CAMI-1981-ENGLISH,MBB/ECKERT
$$ ********************************************
$$ *                                          *
$$ *   THIS TEST OK IN SSV3  -  15-JUN-1989   *
$$ *                                          *
$$ ********************************************
$$
$$   THIS TEST OK IN SSV1 - 15-FEB-1983
$$               AND SSV2 - 02-OCT-1986
$$
PPRINT JOB-FILE- FE353.P202342.SSX8XTS(M0004337)
CLPRNT
$$NOPOST
$$MACHIN/PLARR2,9999
$$MACHIN/CALCOM,9999

NUMPTS/800
MAXDP/500,10000
MAXDP/-4,10
OUTTOL/.005
INTOL/.005
PRINT/LIBPRT,ON
$$PRINT/SSPRT,ON
$$PRINT/SSTEST,ON
PRINT/OFF

PRINT/0

RESERV/P,20

$$ POINTS OF SSURF "SS1"
P(01)=POINT/0,0,0
P(02)=POINT/25,5,5
P(03)=POINT/50,0,0
P(04)=POINT/0,25,5
P(05)=POINT/25,27,10
P(06)=POINT/50,25,5
P(07)=POINT/0,50,0
P(08)=POINT/25,50,5
P(09)=POINT/50,50,0

$$ DEFINITION SSURF "SS1" (SMESH)

SS1=SSURF/SMESH,XYZ,                            $
                    SPLINE,P(01),P(02),P(03),   $
                    SPLINE,P(04),P(05),P(06),   $
                    SPLINE,P(07),P(08),P(09)

$$ POINTS OF SCURVE "C1" UND "C2"

P(10)=POINT/0,0,0
P(11)=POINT/25,5,0
P(12)=POINT/50,0,0

$$ DEFINITION PARM-CURVE

C1=SCURV/SPLINE,P(10),P(11),P(12)

$$ DEFINITION FLOW-CURVE

C2=SCURV/SPLINE,P(10),P(11),P(12),  $
                FLOW,ARC,0,2,       $
                SEG,LENGTH,0,2

$$ INTERSECTION LINE/PARAM-CURVE

P(13)=POINT/50,50,0
P(14)=POINT/25,-25,0
L1=LINE/P(13),P(14)
PC1=POINT/INTOF,L1,C1,P(13)

 E11=PARMF(SVAL)
 E12=PARMF(TVAL)
 E13=PARMF(C1)
 E14=PARMF(C1,PARAM)

$$ INTERSECTION LINE/FLOW-CURVE

PC2=POINT/INTOF,L1,C2,P(13)

 E21=PARMF(SVAL)
 E22=PARMF(TVAL)
 E23=PARMF(C2)
 E24=PARMF(C2,PARAM)
 E25=PARMF(C2,FLOW)

$$ INTERSECTION LINE/SMESH-SURF

P(15)=POINT/30,30,50
V16  =VECTOR/0,0,-1

PS1=POINT/INTOF,SS1,P(15),V16

 E31=PARMF(UVAL)
 E31=PARMF(VVAL)
 E31=PARMF(PVAL)

 E40=PARMF(SS1)

 E51=PARMF(SS1,UPARAM)
 E52=PARMF(SS1,VPARAM)
 E53=PARMF(SS1,PATCH)

$$ TEST OF NUMBER-FUNCTION

D1=NUMF(C1)
D2=NUMF(C2,ARC)
D3=NUMF(C2,SEG)
D4=NUMF(SS1,PATCH)
D5=NUMF(SS1)
D6=NUMF(SS1,SPLINE)
D7=NUMF(SS1,CRSSPL)

STOP
PRINT/3,ALL
FINI
**** FILENAME = [MM001.TESTS]TEST49.DAT        ****
PARTNO/'TEST49'
$$ ********************************************
$$ *                                          *
$$ *   THIS TEST OK IN SSV3  -  23-AUG-1989   *
$$ *                                          *
$$ ********************************************
REMARK/'TOR1 HAS BEEN PREVIOUSLY CREATED BY FTORUS'
PRINT/SSTEST,ON
READ/1,TOR1
M1=MATRIX/TRANSL,10,20,30
TOR2=SSURF/COMBIN,TOR1,TRFORM,M1
M2=MATRIX/XYROT,90
TOR3=SSURF/COMBIN,TOR1,TRFORM,M2
P1=POINT/INTOF,TOR1,PARAM,0.5,0.5,1
P2=POINT/INTOF,TOR2,PARAM,0.5,0.5,1
