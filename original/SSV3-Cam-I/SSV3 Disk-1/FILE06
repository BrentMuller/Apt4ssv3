HL13  =LN/U(1),U(11)
 $$
 $$
APL14 =LN/PAR,AL1,XSMALL,2/A
APL15 =LN/PAR,AL1,XSMALL,4/A
APL16 =LN/PAR,AL1,XSMALL,6/A
APL17 =LN/PAR,AL1,XSMALL,8/A
APL18 =LN/PAR,AL1,XSMALL,10/A
