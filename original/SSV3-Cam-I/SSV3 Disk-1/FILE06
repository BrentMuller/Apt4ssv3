  SPLINE,B7,B6,B5,TS,BV2,B4,TANSPL,BV1,B3,B2,$
  SPLINE,C7,C6,C5,TS,CV2,C4,TANSPL,CV1,C3,C2,$
  SPLINE,D7,D6,D5,TS,DV2,D4,TANSPL,DV1,D3,D2,$
  SPLINE,E7,E6,E5,TS,EV2,E4,TANSPL,EV1,E3,E2


