       DNTCUT
       GD/0,0,.2
       GO/ON,(PL/0,1,0,N),SS10,ON,TAB2
       CUT
       IV/0,1,0
       TLON,GL/(PL/0,1,0,N),ON,TAB1
       DNTCUT
       GD/0,0,.1
       GO/ON,(PL/0,1,0,N),SS1,ON,TAB1
       CUT
       IV/0,1,0
       TLON,GL/(PL/0,1,0,N)
       GL/(PL/0,0,1,-.15)
       GL/(PL/0,1,0,(N-.2)),ON,TAB1
       DNTCUT
       GD/0,0,.2
       GO/ON,(PL/0,1,0,(N-.2)),SS10,ON,TAB1
