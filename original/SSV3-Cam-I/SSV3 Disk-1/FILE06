       GORGT/APL19,ON,HL13
       GODLTA/0,0,4
 $$
 $$ TEST-2  1.0 DIA CUTTER BALL END  OFF SET      TLAXIS/NORMPS
 $$
      GOTO/4.5,6.2,4,0,0,1
       TLAXIS/NORMPS
       CALL/MA
       GODLTA/0,0,4
       TLAXIS/0,0,1
       GOTO/-.55,0,4
       GO/ON,APL19,TO,SS,ON,HL12
       TLON,GOLFT/APL19
       GORGT/HL13
       GORGT/APL18
       GOLFT/HL12
