      BLOCK DATA SSDAT1
      IMPLICIT DOUBLE PRECISION(A-H,O-Z)
      COMMON/SSARLM/B(64,4),U(4),V(4),ITOP(6,4,4),IFLAG(4,4),IPNUM(4),
     X  ISEQLS,NLAST,MCAT,JENLAS(4),MODLAS(4),IRRLAS(4),NCALMX,NCMX
      COMMON/IBUGG/IBUG,IPCOLC,IPCOLM
