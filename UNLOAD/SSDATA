      DATA MODLAS,IRRLAS,JENLAS,IPNUM,NCALMX,NCMX/17*0,150/
      DATA ISEQLS,NLAST,MCAT/-1,-1,0/
C---     NSCNMX IS THE MAX SPACE IN SCAN, ISCAN THE LAST USED LOCATION
      DATA NSCNMX/ISCN/
      DATA ISCAN/0/
      DATA SCAN/ISCN*0.0D0/
      DATA DGEOM/4*0.D0/
      DATA DLIM/4*0.D0/
      DATA TLDIR/3*0.D0/
      DATA DOFSET/0.D0/
      DATA ITLDIR/0/
      DATA IDTOOL/0/
      DATA NDS/-121/
      DATA PGEOM/4*0.D0/
      DATA PSIDE/1.0D0/
      DATA PTHICK/0.D0/
      DATA IPTOOL/0/
      DATA NPS/-121/
      DATA CLRPLN/4*0.D0/
      DATA FED/4*0.D0/
      DATA NCLRPL/-121/
      DATA NFEED/-121/
      DATA SSTEP/4*0.D0/
      DATA NSTEPO/-121/
      DATA AXIS/3*0.D0/
      DATA IAXIS/0/
      DATA NAXIS/-121/
      END
